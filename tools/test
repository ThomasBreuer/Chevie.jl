

CHEVIE.AddData("UnipotentClasses","E8",function(p)local uc,Z,l,l1,i,s,c,class

  if p==0   p=1 end
Z=n->ComplexReflectionGroup(n,1,1)

  class=n->First(uc.classes,x->x.name==n)

